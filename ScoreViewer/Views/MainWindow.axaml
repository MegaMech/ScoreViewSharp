<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:views="clr-namespace:ScoreViewer.Views"
		xmlns:vm="clr-namespace:ScoreViewer.ViewModels"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" d:DesignWidth="600" d:DesignHeight="250"
        x:Class="ScoreViewer.Views.MainWindow"
        Icon="/Assets/avalonia-logo.ico"
        Title="ScoreViewer"
		ExtendClientAreaToDecorationsHint="True"
		ExtendClientAreaChromeHints="NoChrome"
		ExtendClientAreaTitleBarHeightHint="-1"
		x:DataType="vm:MainViewModel">

	<Grid>
			<views:MenuView DataContext="{Binding MenuViewModel}"/>
	</Grid>
</Window>